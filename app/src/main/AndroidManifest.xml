<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roberto.heartratemonitor">
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <application
        android:icon="@mipmap/dog"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.roberto.petfeedmonitor.SelectBluetooth"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.roberto.petfeedmonitor.ScanBluetooth" />
        <activity android:name="com.example.roberto.petfeedmonitor.MainPage" />
    </application>
</manifest>